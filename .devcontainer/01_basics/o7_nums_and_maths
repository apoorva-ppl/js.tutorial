const score=400
console.log(score);

const balance= new Number(100)
console.log(balance);

console.log(balance.toString().length()); //3(no. of characyters in the strings)
console.log(balance.toFixed(2)); //"100.00"( converts number into strings) //2 represents the number after decimal

const otherNumber = 23.8966
console.log(otherNumber.toPrecision(3)); //"23.9"